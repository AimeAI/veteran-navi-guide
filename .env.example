# Supabase Configuration
# Get these from your Supabase project settings: https://app.supabase.com
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Feature Flags
VITE_ENABLE_MILITARY_SKILLS_MATCHER=true
VITE_ENABLE_ADVANCED_SEARCH=true
VITE_ENABLE_REAL_TIME_NOTIFICATIONS=false

# API Configurations
VITE_API_TIMEOUT_MS=30000
VITE_MAX_API_RETRIES=3
VITE_API_RATE_LIMIT=60

# Cache Settings
VITE_CACHE_TIME_MS=300000

# Debug Settings (set to false in production)
VITE_DEBUG=true
VITE_LOG_LEVEL=debug
